        "from sklearn.tree import DecisionTreeClassifier\n",
        "\n",
        "# valor, categoria (0=alimentação, 1=lazer)\n",
        "X = [[50,0],[200,1],[30,0],[400,1]]\n",
        "y = [1,0,1,0]  # 1 essencial, 0 supérfluo\n",
        "\n",
        "model = DecisionTreeClassifier()\n",
        "model.fit(X,y)\n",
        "model.predict([[120,1]])"
      ]
    }
